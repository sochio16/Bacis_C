#include<stdio.h>
int main(){
    
    int a = 13, b = 4, integer_result;
    double c = 13.0, d = 4.0, double_result;
    
    integer_result = a / b;
    double_result  = a / b;
    
    printf("integer_result = %d\n", integer_result);
    printf("double_result  = %lf", double_result);
    
    
    return 0;
}